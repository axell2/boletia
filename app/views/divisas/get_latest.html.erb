<%= @latoos%>
<h1>Ultimos precios en la plataforma</h1>
<h2>Ultima actualizacion <%= @meta%></h2>
<br>
<table class="table">
  <thead>
    <tr>
      <th>Code</th>
      <th>Value</th>
      <th>created_at</th>
      <th>Updated_at</th>
      <th>timestamp de plataforma</th>
    
    </tr>
  </thead>
  <tbody>
    <%@latest.each do |currency|%>
      <tr>
        <td><%=currency.code%></td>
        <td><%=currency.value%></td>
        <td><%=currency.created_at %></td>
        <td><%=currency.updated_at %></td>
        <td><%=currency.timestamp%></td>
      </tr>
    <%end%>
  </tbody>
</table>

